<div class="container container-left-align">
  <div class="main-page col-md-5 col-sm-12 col-xs-12">
    <ul class="nav nav-tabs">
      <li class="active">
        <a data-toggle="tab" href="#articles">Articles</a>
      </li>
      <li>
        <a data-toggle="tab" href="#videos">Videos</a>
      </li>
    </ul>

    <div class="tab-content">
      <div id="articles" class="tab-pane fade in active">
        <ol>
          <% @articles.each do |post| %>
              <li>
                <%= post.postable.headline %></br>
              </li>
          <% end %>
        </ol>
      </div>
      <div id="videos" class="tab-pane fade">
        <ol>
          <% @videos.each do |post| %>
              <li>
                <%= post.postable.name %></br>
              </li>
          <% end %>
        </ol>

      </div>
    </div>
  </div>
  <div class="col-md-3">
  </div>
  <div class="col-md-4">
  </div>
</div>
<%#= will_paginate @videos %>
<%#= will_paginate @articles %>